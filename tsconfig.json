{
    "compilerOptions": {
        "target": "ES2020",
        "lib": ["ES2020", "DOM"],
        "module": "commonjs",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "types": ["cypress", "node"],
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "strict": false
    },
    "include": ["cypress", "cypress.config.ts"]
}